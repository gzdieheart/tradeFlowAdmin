  "homepage": "https://vuejs-core.cn",
  "scripts": {
    "serve:rspack": "node --no-deprecation rspack.js serve",
    "build": "node --no-deprecation rspack.js build",
    "print-info": "node -e \"require('./layouts').donationConsole()\"",
    "clear": "rimraf node_modules&&npm install  --registry=--registry=https://registry.npmmirror.com",
    "update": "ncu -u --reject chalk,sass,sass-loader --registry=https://registry.npmmirror.com&&pnpm i",
    "push": "start ./push.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zxwk1998/vue-admin-better.git"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "git add"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.11.0",
    "caniuse-lite": "^1.0.30001727",
    "clipboard": "^2.0.11",
    "core-js": "^3.44.0",
    "dayjs": "^1.11.13",
    "echarts": "5.6.0",
    "element-plus": "^2.10.4",
    "jsencrypt": "^3.3.2",
    "layouts": "file:layouts",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "qs": "^6.14.0",
    "screenfull": "^6.0.2",
